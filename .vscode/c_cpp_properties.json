            "compilerArgs": [],
            "mergeConfigurations": false,
            "browse": {
                "path": [
                    "${workspaceFolder}/incs",
                    "${workspaceFolder}/libft",
                    "${workspaceFolder}/structure/**",
                    "${workspaceFolder}/structure/scene/",
                    "${workspaceFolder}/trace/**",
                    "${workspaceFolder}"
